example:
#insert sort:
arr = [1,4,3,5,6,2]
n = len(arr)
for i in range(1,n):
        for j in range(i):
            if arr[j] > arr[i]:
                arr[i], arr[j] = arr[j], arr[i]
        print(*arr)
        
#
a = [1, 2, 3, 4, 5]
 
# printing the list using * operator separated
# by comma
print(*a)
1 2 3 4 5
# printing the list using * and sep operator
print("printing lists separated by commas")
 
print(*a, sep = ", ")
 1, 2, 3, 4, 5
# print in new line
print("printing lists in new line")
 
print(*a, sep = "\n")
1
2
3
4
5
